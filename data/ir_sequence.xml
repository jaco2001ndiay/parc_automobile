<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence pour les cartes grises -->
        <record id="seq_carte_grise" model="ir.sequence">
            <field name="name">Carte Grise</field>
            <field name="code">parc.automobile.carte.grise</field>
            <field name="prefix">CG-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence pour les contrats de location -->
        <record id="seq_contrat_location" model="ir.sequence">
            <field name="name">Contrat de Location</field>
            <field name="code">parc.automobile.contrat.location</field>
            <field name="prefix">CT-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence pour les entretiens -->
        <record id="seq_entretien" model="ir.sequence">
            <field name="name">Entretien</field>
            <field name="code">parc.automobile.entretien</field>
            <field name="prefix">ENT-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>